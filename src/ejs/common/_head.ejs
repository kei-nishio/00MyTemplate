<head>
  <!-- ! noindex and nofollow -->
  <meta name="robots" content="noindex, nofollow" />

  <!-- meta（不変）情報 -->
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta name="format-detection" content="telephone=no" />

  <!-- meta情報 -->
  <title><%= page['title'] %></title>
  <meta name="description" content="<%= page['description'] %>" />

  <!-- OGP(Open Graph Protocol) -->
  <meta property="og:type" content="website か description か article" />
  <meta property="og:title" content="<%= page['title'] %>" />
  <meta property="og:description" content="<%= page['description'] %>" />
  <meta property="og:url" content="<%= page['url'] %>" />
  <meta property="og.image" content="<%= baseUrl %>/assets/images/common/ogp.jpg" />
  <meta property="og:image:width" content="1200" />
  <meta property="og:image:height" content="630" />
  <meta property="og:locale" content="ja_JP" />
  <meta property="og:site_name" content="<%= page['title'] %>" />
  <meta name="twitter:card" content="summary_large_image" />

  <!-- X OGP -->
  <meta name="twitter:card" content="summary_large_image" />
  <meta name="twitter:title" content="<%= page['title'] %>" />
  <meta name="twitter:description" content="<%= page['description'] %>" />
  <meta name="twitter:image" content="<%= baseUrl %>/assets/images/common/ogp.jpg" />

  <!-- favicon -->
  <link rel="icon" href="<%= baseUrl %>/assets/images/common/favicon.png" />
  <!-- <link rel="apple-touch-icon" href="<%= baseUrl %>/images/apple-touch-icon.png" sizes="180*180" /> -->

  <!-- canonical -->
  <link rel="canonical" href="このページのURL" />

  <!-- google font -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Noto+Sans+JP:wght@100..900&display=swap" rel="stylesheet" />
  <link href="https://fonts.googleapis.com/css2?family=Gotu&family=Lato:wght@400;700&family=Noto+Sans+JP:wght@400;500;700&display=swap" rel="stylesheet">

  <!-- css -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.css" />
  <link rel="stylesheet" href="<%= baseUrl %>/assets/css/ress.min.css" />
  <link rel="stylesheet" href="<%= baseUrl %>/assets/css/style.min.css" />

  <!-- JavaScript -->
  <script defer src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.5/gsap.min.js" integrity="sha512-7eHRwcbYkK4d9g/6tD/mhkf++eoTHwpNM9woBxtPUBWm67zeAfFC+HrdoE2GanKeocly/VxeLvIqwvCdk7qScg==" crossorigin="anonymous"></script>
  <script defer src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.5/ScrollTrigger.min.js" integrity="sha512-onMTRKJBKz8M1TnqqDuGBlowlH0ohFzMXYRNebz+yOcc5TQr/zAKsthzhuv0hiyUKEiQEQXEynnXCvNTOk50dg==" crossorigin="anonymous"></script>
  <script defer src="<%= baseUrl %>/assets/js/script.js"></script>
</head>